@model List<DocumentNonOfficiel>
@using ISCAE.Data
@{
    ViewBag.Title = "Document > Non Officiel";


    if(Session["user"] is Etudiant)
    {
        Layout = "~/Views/Shared/_EtudiantLayout.cshtml";
        @Html.Partial("NonOfficielEtudiant", Model);
    }
    else if (Session["user"] is Professeur)
    {
        Layout = "~/Views/Shared/_ProfesseurLayout.cshtml";
        @Html.Partial("NonOfficielProfesseur", Model);
    }
}
