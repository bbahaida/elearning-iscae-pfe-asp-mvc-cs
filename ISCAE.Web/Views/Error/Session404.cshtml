@model string
@using ISCAE.Data
@{
    ViewBag.Title = "Not Found";
    string url = "";
    if (Session["user"] is Etudiant)
    {
        Layout = "~/Views/Shared/_EtudiantLayout.cshtml";
        url = Url.Action("Index", "Etudiant");
    }
    else if(Session["user"] is Professeur)
    {
        Layout = "~/Views/Shared/_ProfesseurLayout.cshtml";
        url = Url.Action("Index", "Professeur");
    }
    else
    {
        Layout = "~/Views/Shared/_AdministrateurLayout.cshtml";
        url = Url.Action("Index", "Administrateur");
    }

}

<div class="error-page">
    <h2 class="headline text-yellow"> 404</h2>
    <div class="error-content">
        <h3><i class="fa fa-warning text-yellow"></i> Oops! Page @Model not found.</h3>
        <p>
            We could not find the page you were looking for.
            Meanwhile, you may <a href="@url">return to dashboard</a> or try using the search form.
        </p>
        <form class="search-form">
            <div class="input-group">
                <input type="text" name="search" class="form-control" placeholder="Search">
                <div class="input-group-btn">
                    <button type="submit" name="submit" class="btn btn-warning btn-flat">
                        <i class="fa fa-search"></i>
                    </button>
                </div>
            </div>
            <!-- /.input-group -->
        </form>
    </div>
    <!-- /.error-content -->
</div>
<!-- /.error-page -->